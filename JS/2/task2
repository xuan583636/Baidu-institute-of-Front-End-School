<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>IFE JavaScript Task 01</title>
</head>
<body>

    <h3>污染城市列表</h3>
    <ul id="aqi-list">
        <!--
            <li>第一名：福州（样例），10</li>
              <li>第二名：福州（样例），10</li> -->
    </ul>

    <script type="text/javascript">

        var aqiData = [
            ["北京", 90],
            ["上海", 50],
            ["福州", 10],
            ["广州", 50],
            ["成都", 90],
            ["西安", 100]
        ];

        function compare(num1, num2) {
            return num1[1] - num2[1];
        }

        function list(a) {
            var new_object = a.sort(compare);
            
        }

        function addList() {
            var unorderList = document.getElementById("aqi-list");
            for (var i = 0; i < aqiData.length; i++) {
                var listArray = new Array();
                listArray[i] = document.createElement("li");
                listText = document.createTextNode("第" + (i + 1) + "名: " + aqiData[i][0] + "," + aqiData[i][1]);
                listArray[i].appendChild(listText);
                document.getElementById("aqi-list").appendChild(listArray[i]);


            }
            

        }

        list(aqiData);
        addList();



    </script>
</body>
</html>
